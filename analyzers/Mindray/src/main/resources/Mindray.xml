<?xml version="1.0" encoding="UTF-8"?>
<openElisGlobalPlugin>
<version>1.0</version>
<analyzerImporter>
<extension_point path="org.openelisglobal.plugin.AnalyzerImporterPlugin" >
    <extension path="oe.plugin.analyzer.MindrayAnalyzer" />   
    <description value="Mindray" />
</extension_point>
</analyzerImporter>
<menu>
    <extension_point path="org.openelisglobal.plugin.MenuPlugin" >
    <extension path="oe.plugin.analyzer.MindrayMenu" />   
    <description value="Menu for Mindray" />
</extension_point>
</menu>
<permission>
    <extension_point path="org.openelisglobal.plugin.PermissionPlugin" >
        <extension path="oe.plugin.analyzer.MindrayPermission" />
        <description value="Mindray Analyzer permission" />
    </extension_point>
</permission>
<servlet>
    <extension_point path="org.openelisglobal.plugin.ServletPlugin" >
        <extension path="oe.plugin.analyzer.MindrayServlet" />
        <servlet-path value="/MindrayServlet" />
        <servlet-name value="MindrayServlet" />
        <description value="Mindray Analyzer servlet" />
    </extension_point>
</servlet>
</openElisGlobalPlugin>
